<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- Device model (Build.MODEL) to spoof for netflix, if not empty -->
    <java-symbol type="string" name="config_netflixSpoofModel" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Proximity check on wake -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />
    <java-symbol type="integer" name="config_proximityCheckTimeout" />
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

    <!-- Whether to use Richtap vibration -->
    <java-symbol type="bool" name="config_usesRichtapVibration" />

    <!-- Whether Pixel props is enabled -->
    <java-symbol type="bool" name="config_enablePixelProps" />

    <!-- Whether device has Google Tensor SoC -->
    <java-symbol type="bool" name="config_hasTensorSoC" />

    <!-- Whether the device supports Smart Pixels -->
    <java-symbol type="bool" name="config_supportSmartPixels" />

    <!-- Title & summary of notification letting users know that battery saver is now on -->
    <java-symbol type="string" name="battery_saver_on_notification_title" />
    <java-symbol type="string" name="battery_saver_on_notification_summary" />

    <!-- Display color inversion availability -->
    <java-symbol type="bool" name="config_displayInversionAvailable" />

    <!-- Build properties from a GMS certified device -->
    <java-symbol type="array" name="config_certifiedBuildProperties" />

    <!-- Pocket Lock -->
    <java-symbol type="layout" name="pocket_lock_view" />

    <!-- Pocket bridge -->
    <java-symbol type="string" name="config_pocketBridgeSysfsInpocket" />
    <java-symbol type="bool" name="config_pocketModeSupported" />
    <java-symbol type="bool" name="config_pocketUseLightSensor" />

    <!-- Pocket Service -->
    <java-symbol type="string" name="config_pocketJudgeVendorSensorName" />
</resources>
